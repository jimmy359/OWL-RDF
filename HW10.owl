<?xml version="1.0"?>
<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" 
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
         xml:base="http://csie.nfu.edu.tw/BGC0303/40843244">

    <owl:Class rdf:ID="Image_Recognition"></owl:Class>

    <owl:Class rdf:ID="Image_Dataset">
        <rdfs:subClassOf rdf:resource="#Image_Recognition"/>
    </owl:Class>

    <owl:Class rdf:ID="Model">
        <rdfs:subClassOf rdf:resource="#Image_Recognition"/>
    </owl:Class>

    <owl:Class rdf:ID="File">
        <rdfs:subClassOf rdf:resource="#Image_Dataset"/>
    </owl:Class>

    <owl:Class rdf:ID="Label">
        <rdfs:subClassOf rdf:resource="#Image_Dataset"/>
    </owl:Class>

    <owl:Class rdf:ID="Model_Architecture">
        <rdfs:subClassOf rdf:resource="#Model"/>
    </owl:Class>

    <owl:Class rdf:ID="Training_Method">
        <rdfs:subClassOf rdf:resource="#Model"/>
    </owl:Class>

    <owl:Class rdf:ID="Training_Image">
        <rdfs:subClassOf rdf:resource="#File"/>
    </owl:Class>

    <owl:Class rdf:ID="Test_Image">
        <rdfs:subClassOf rdf:resource="#File"/>
    </owl:Class>

    <owl:SymmetricProperty rdf:ID="Similarity">
        <rdfs:domain rdf:resource="#Test_Image"/>
        <rdfs:range rdf:resource="#Test_Image"/>
    </owl:SymmetricProperty>

    <owl:ObjectProperty rdf:ID="Belong">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseProperty"/>
        <owl:InverseOf rdf:resource="#Include"/>
        <rdfs:domain rdf:resource="#File"/>
        <rdfs:range rdf:resource="#Training_Image"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="Belong_To">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#File"/>
        <rdfs:range rdf:resource="#Label"/>
    </owl:ObjectProperty>

</rdf:RDF>
